   "source": [
    "x = sy.symbols('x')\n",
    "z = sy.symbols('z', real=True, positive=True)\n",
    "\n",
    "gamma_integrand = x**(z-1)*sy.exp(-x)\n",
    "sy.integrate(gamma_integrand,(x,0,sy.oo))"
   ]
  },
